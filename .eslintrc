extends:
  - standard
  - standard-react
  - plugin:jsx-a11y/recommended
plugins:
  - jsx-a11y
parser: babel-eslint
rules:
  object-curly-spacing: [ error, always ]
  react/prop-types: off
  react/jsx-filename-extension: off
  space-before-function-paren: off
  jsx-quotes: off
globals:
  React: false
